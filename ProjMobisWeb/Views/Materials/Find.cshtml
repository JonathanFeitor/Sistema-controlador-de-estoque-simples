@model Model.Entity.Materials

@{
    ViewBag.Title = Model.MType;
}

<ol class="breadcrumb">
    <li><a href="~/">Materials</a></li>
    <li class="active">@(Model.MType)</li>
</ol>
<div class="panel panel-default">
    <div class="panel-heading">Materials information</div>
    <div class="panel-body">
        <dl class="dl-horizontal">
            <dt>MCode</dt>
            <dd>@Model.MCode</dd>

            <dt>Type</dt>
            <dd>@Model.MType</dd>

            <dt>Model</dt>
            <dd>@Model.MModel</dd>

            <dt>Quantity</dt>
            <dd>@Model.MQuantity</dd>

            <dt>Note</dt>
            <dd>@Model.MObservation</dd>

            <dt>User ID</dt>
            <dd>@Model.MIdOperator</dd>

            <dt>Date</dt>
            <dd>@Model.MDate</dd>

        </dl>
    </div>
    <div class="form-actions">
        |@Html.ActionLink("Return to list", "Index")
    </div>
</div>
